<p id="notice"><%= notice %></p>

<h1>Outputs</h1>

<table>
  <thead>
    <tr>
      <th>Output name</th>
      <th>Output type</th>
      <th>Transformation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outputs.each do |output| %>
      <tr>
        <td><%= output.output_name %></td>
        <td><%= output.output_type %></td>
        <td><%= output.transformation %></td>
        <td><%= link_to 'Show', output %></td>
        <td><%= link_to 'Edit', edit_output_path(output) %></td>
        <td><%= link_to 'Destroy', output, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Output', new_output_path %>
